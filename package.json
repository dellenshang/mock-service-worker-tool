{
  "name": "mock-service-worker-tool",
  "version": "1.0.0",
  "description": "use mock service worker in your App with TS",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "usemsw": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dellenshang/mock-service-worker-tool.git"
  },
  "keywords": [
    "mock",
    "service",
    "worker",
    "mock-service-worker",
    "msw"
  ],
  "author": "dellenshang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dellenshang/mock-service-worker-tool/issues"
  },
  "homepage": "https://github.com/dellenshang/mock-service-worker-tool#readme",
  "devDependencies": {
    "prettier": "^2.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "msw": "^0.35.0"
  }
}
